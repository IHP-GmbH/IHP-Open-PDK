 layer via3large via3
	shrink 710
	grow 710

 layer VIA3 via3
	and-not via3large
	and-not sealring
	squares-grid 5 190 220
	calma 49 0

 layer VIA3 via3
	and via3large
	and-not sealring
 	squares-grid 5 190 290
