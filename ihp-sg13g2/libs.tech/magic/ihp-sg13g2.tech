# VIA5 (TopVia1)
#----------------------------------------------------------------

 layer VIA5	via5
	and-not sealring
 	squares-grid 5 420 420
 	calma 	125 0

 layer VIA5	sealv5
	and sealring
	calma	125 0

 layer MIMCC	mimcc,mimrfcc
 	squares-grid 360 420 420
 	calma 	129 0

