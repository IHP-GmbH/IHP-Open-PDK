 spacing hvisodiffres *pdiff,*hvpdiff 690 touching_illegal \
	"isodiffres spacing to P-diffusion < %d (nSDB.c + overlaps)"
 spacing hvisodiffres *psd,*hvpsd 540 touching_ok \
	"isodiffres spacing to P+ diffusion < %d (nSDB.c + overlaps)"

