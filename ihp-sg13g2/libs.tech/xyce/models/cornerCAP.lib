**************** CORNERS OF CAPACITORS ****************  
* Typical without statistical modeling          
.LIB cap_typ  
.param cap_carea = 1.5E-15
.param cap_cpara  = 1.0
           
.include capacitors_mod.lib
.ENDL cap_typ
  
* Typical with statistical modeling
.LIB cap_typ_stat  
.param cap_carea_norm = 1.5E-15
.param cap_cpara_norm  = 1.0
           
.include capacitors_stat.lib
.include capacitors_mod.lib
.ENDL cap_typ_stat

* Best Case without statistical modeling
.LIB cap_bcs  
.param cap_carea = 0.9*1.5E-15
.param cap_cpara  = 0.9
           
.include capacitors_mod.lib
.ENDL cap_bcs
      
* Best Case with statistical modeling
.LIB cap_bcs_stat  
.param cap_carea_norm = 0.9*1.5E-15
.param cap_cpara_norm  = 0.9

           
.include capacitors_stat.lib
.include capacitors_mod.lib
.ENDL cap_bcs_stat
      
* Worst Case without statistical modeling
.LIB cap_wcs 
.param cap_carea = 1.1*1.5E-15
.param cap_cpara  = 1.1
           
.include capacitors_mod.lib
.ENDL cap_wcs
      
* Worst Case with statistical modeling
.LIB cap_wcs_stat  
.param cap_carea_norm = 1.1*1.5E-15
.param cap_cpara_norm  = 1.1
           
.include capacitors_stat.lib
.include capacitors_mod.lib
.ENDL cap_wcs_stat
