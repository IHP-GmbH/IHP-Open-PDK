* ngspice mismatch parameters
.param sg13g2_lv_nmos_delvto_mm  = 0.004
.param sg13g2_lv_nmos_factuo_mm  = 0.01
.param sg13g2_lv_nmos_dw_mm      = 5e-9
.param sg13g2_lv_nmos_dl_mm      = 5e-9

.param sg13g2_lv_pmos_delvto_mm  = 0.032
.param sg13g2_lv_pmos_factuo_mm  = 0.01
.param sg13g2_lv_pmos_dw_mm      = 5e-9
.param sg13g2_lv_pmos_dl_mm      = 5e-9

* spectre mismatch section
*mismatch {   
*  vary sg13g2_lv_nmos_vfbo_mm     dist=gauss  std=0.0040 percent=no  
*
*  vary sg13g2_lv_pmos_vfbo_mm     dist=gauss  std=0.0032 percent=no  
*
*  vary sg13g2_lv_svaricap_vfbo_mm dist=gauss  std=0.0001 percent=no         
*}
*truncate tr=3 // +/- 3 Sigma 